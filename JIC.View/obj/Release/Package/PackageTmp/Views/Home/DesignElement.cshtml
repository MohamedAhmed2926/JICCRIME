@using JIC.Base.Views

@model IEnumerable<vw_UserData>

@{
    ViewBag.Title = "DesignElement";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DesignElement</h2>


@(Html
        .Grid(Model)
        .Build(columns =>
        {
            columns.Add(model => Html.CheckBox("_check" + model.ID)).Titled("ID");
            columns.Add(model => model.UserName).Titled("Name");
        })
        .Filterable()
        .Sortable()
        .Pageable()
)

