@using JIC.Crime.View.Models
@model  ProsecutorViewModels
@if (ViewData["SessionEnded"] != null && (bool)ViewData["SessionEnded"])
{
    <script>
        debugger;
        window.location.href = "/User/Login?returnUrl=/";
    </script>
}


<div id="DeleteDiv">
    @using (Ajax.BeginForm("Delete", new AjaxOptions { HttpMethod = "Post", UpdateTargetId = "DeleteDiv" }))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(model => model.ID)

        @Html.HiddenFor(model => model.ProcecutoerName)
        @Html.HiddenFor(model => model.ProsecutionName)
        @Html.HiddenFor(model => model.NationalID)
        <div class="row">
            <div class="col-sm-3">
                @Html.LabelFor(m => m.ProcecutoerName)
            </div>
            <div class="col-sm-3">
                @Html.LabelFor(m => m.ProcecutoerName, Model.ProcecutoerName)
            </div>
        </div>
        <div class="row">
            <div class="col-sm-3">
                @Html.LabelFor(m => m.ProcecutionID)
            </div>
            <div class="col-sm-3">
                @Html.LabelFor(m => m.ProcecutionID, Model.ProsecutionName)
            </div>
        </div>
        <div class="row">
            <div class="col-sm-3">
                @Html.LabelFor(m => m.NationalID)
            </div>
            <div class="col-sm-3">
           
                  @if (Model.NationalID != null)
                  {
             @Html.LabelFor(m => m.NationalID, Model.NationalID)
                      }
                 
            </div>
        </div>
        <div class="row">
            <div class="col-sm-3">
                <input type="submit" value="@JIC.Base.Resources.Resources.Delete" id="Delete" class="btn btn-default" name="action:Delete" />
            </div>
            <div class="col-sm-3">
                @*   <input type="submit" value="@JIC.Base.Resources.Resources.Cancel" name="action:CancelDelete" id="Cancel" class="btn btn-default" formaction="CancelDelete" />*@
                <button type="button" class="btn btn-default" data-dismiss="modal">@JIC.Base.Resources.Resources.Cancel</button>
            </div>
        </div>
    }
</div>

