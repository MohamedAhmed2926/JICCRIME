@using JIC.Crime.View.Models
@model  ProsecutorCreateViewModel
@if (ViewData["SessionEnded"] != null && (bool)ViewData["SessionEnded"])
{
    <script>
        debugger;
        window.location.href = "/User/Login?returnUrl=/";
    </script>
}

<div id="Creatediv">
    <h2>@JIC.Base.Resources.Resources.ProsecutorAdd</h2>
    @using (Ajax.BeginForm("Create", new AjaxOptions { HttpMethod = "Post", UpdateTargetId = "Creatediv" }))
    {<div class="row">
            @Html.AntiForgeryToken()
            <div class="col-lg-4">
                @Html.CTextBoxFor(m => m.ProsecutorModel.ProcecutoerName)
            </div>
            <div class="col-lg-4">
                @Html.CDropDownFor(m => m.ProsecutorModel.ProcecutionID, new SelectList(Model.ListProsecutionModel, "ID", "ProsecutionName"), true)
            </div>
            <div class="col-lg-4">
                @Html.CTextBoxFor(m => m.ProsecutorModel.NationalID)
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4">
                <input type="submit" value="@JIC.Base.Resources.Resources.prosecuteradd" id="ADD" class="btn btn-default" />
            </div>
            <div class="col-sm-4">
                <input type="reset" value="@JIC.Base.Resources.Resources.Reset" class="btn btn-default" />

            </div>
        </div>
    }
</div>